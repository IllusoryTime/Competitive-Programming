#include <bits/stdc++.h>
using namespace std;

int main(){
    //freopen("in.txt", "r", stdin);
    int t, n, tc = 0; char str[10];
    cin >> t;
    while(t--){
        printf("Case %d: ", ++tc);
        cin >> n >> str;
        if(str[0] == 'A'){
            if(n%3 == 1) cout << "Bob\n";
            else cout << "Alice\n";
        }
        else if(str[0] == 'B'){
            if(n%3 == 0) cout << "Alice\n";
            else cout << "Bob\n";
        }
    }
    return 0;
}
